<% provide(:title, "Homepage") %>
<style></head><body><h1> style="text-align: center; font-family: Abyssinica SIL;">Fotasia Hompage<br>
</h1></body></style>
<div style="text-align: center;"><img src="/images/fotasia.jpg" alt="Fotasia Album Website" style="width: 286px; height: 286px;" align="top">
  <!DOCTYPE html>
</div>
<!-- Page Content -->
<div class="container">

  <div class="row">

    <div class="col-lg-12">
      <h1 class="page-header">Foto Galery</h1>
    </div>
        <% foto_all.each do |foto|%>
            <div class="col-lg-3 col-md-4 col-xs-6 thumb">
              <a class="thumbnail" href=<%=foto.image.url%> data-toggle="tooltip" title="User:  <%= foto.username %>
      &#10;Title:  <%= foto.title %>
     &#10;Date_Created:  <%= foto.date %>">
                <img class="img-responsive" src=<%= foto.image%>>
                <p><%= foto.content %></p>
              </a>
            </div>
        <% end %>
    <hr>

    <!-- Footer -->
    <footer>
      <div class="row">
        <div class="col-lg-12">
          <p>Copyright &copy; K.O Website 2015</p>
        </div>
      </div>
    </footer>
  </div>
</div>
<!-- /.container -->